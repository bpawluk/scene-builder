<Window x:Class="SceneBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SceneBuilder"
        xmlns:view="clr-namespace:SceneBuilder.View"
        mc:Ignorable="d"
        Title="Scene Builder" ResizeMode="NoResize" WindowState="Maximized" WindowStyle="None">
    <Window.Resources>
        <view:Workspace x:Key="Home"/>
    </Window.Resources>
    
    <Frame Name="WindowContentFrame" Content="{StaticResource Home}" NavigationUIVisibility="Hidden"/>
</Window>
